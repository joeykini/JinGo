	<array>
		<string>allow-vpn</string>
	</array>

	<!-- App Groups - 用于与主APP共享数据 -->
	<key>com.apple.security.application-groups</key>
	<array>
		<string>group.work.opine.jingo</string>
	</array>

	<!-- Keychain Access Groups - 用于安全存储 -->
	<key>keychain-access-groups</key>
	<array>
		<string>6HP2RFA5AK.*</string>
		<string>com.apple.token</string>
	</array>

	<!-- Allow get-task-allow for debugging -->
	<key>get-task-allow</key>
